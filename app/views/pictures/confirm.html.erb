<%= form_with(model: @picture, local: true) do |f| %>
<%= f.hidden_field :content %>
<%= f.hidden_field :image_cache %>
<%= f.label :content %>
<%= @picture.content %>
<%= f.label :image %>
<%= image_tag @picture.image.url %>
<%= f.submit "送信" , class:"btn btn-outline-dark btn-sm" %>
<%= f.submit "戻る", name:"back" , class:"btn btn-outline-dark btn-sm" %>
<% end %>
