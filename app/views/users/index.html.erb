<div class="">
  <% @users.each do |user| %>
  <div class="">
    <% if user.avatar.url.nil? %>
      <%= image_tag 'f_f_event_75_s128_f_event_75_1bg.png'%>
    <% else %>
      <%= image_tag user.avatar.url %>
    <% end %>
  </div>
  <div class="">
    <%= user.name %>
  </div>
  <% end %>
</div>
