<%= form_with(model: @picture, local: true) do |f| %>
  <% if @picture.errors.any? %>
  <div class="">
    <% @picture.errors.full_messages.each do |message| %>
    <p><%= message %></p>
    <% end %>
  </div>
  <% end %>
<%= f.label :content %>
<%= f.text_field :content %>
<%= f.label :image %>
<%= f.file_field :image %>
<%= f.submit "送信" %>
<% end %>
